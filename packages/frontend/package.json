{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run format:check && npm run lint:check && vue-tsc -b && vite build",
    "preview": "vite preview",
    "shadcn-vue": "shadcn-vue",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-query": "^5.81.5",
    "@vueuse/core": "^13.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eventsource-parser": "^3.0.3",
    "lucide-vue-next": "^0.525.0",
    "reka-ui": "^2.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.4",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@gitlab/eslint-plugin": "^21.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^9.33.0",
    "prettier": "^3.6.1",
    "shadcn-vue": "^2.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^7.0.0",
    "vue-tsc": "^2.2.10"
  },
  "note": "This is a workaround for https://github.com/unovue/shadcn-vue/issues/1319",
  "overrides": {
    "css-select": "5.1.0"
  }
}
